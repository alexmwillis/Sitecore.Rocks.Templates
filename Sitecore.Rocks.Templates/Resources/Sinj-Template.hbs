var {{#removeWhiteSpace Name}}Template: SinjTemplateDto = {
    id: "{{Id}}",{{#newline}}
    name: "{{Name}}",{{#newline}}
    template: "{{TemplatePath}}",{{#newline}}
    parent: "{{ItemPath}}",{{#newline}}
    language: "en",{{#newline}}
    fields: {
        "Icon": {{Icon}},{{#newline}}
        "Base template": {{BaseTemplateList}}{{#newline}}
    },{{#newline}}
    sections: [
{{#each Sections}}
        {
{{#ifNotFirst ','}}
            name: {{Name}},
            id: {{Id}},
            sectionFields: {
                "Icon": {{Icon}},{{#newline}}
            }
            fields: [
{{#each Fields}}
{{#ifNotFirst ','}}{{#newline}}
            {
                id: {{Id}},{{#newline}}
                name: {{Name}},{{#newline}}
                type: {{Type}},{{#newline}}
                sortorder: {{SortOrder}},{{#newline}}
            }
{{/each}}{{#newline}}
        }
    ],{{#newline}}
    standardValues: TODO
{{/each}}{{#newline}}