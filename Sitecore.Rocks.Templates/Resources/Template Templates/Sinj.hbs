var {{#camelCase Name}}Template: SinjTemplateDto = { {{#newline}}
    id: "{{Id}}",{{#newline}}
    name: "{{Name}}",{{#newline}}
    parent: "{{ParentPath}}",{{#newline}}
    fields: { {{#newline}}
        icon: "{{Icon}}",{{#newline}}
        "Base template": "{{BaseTemplateList}}"{{#newline}}
    },{{#newline}}
    sections: [
{{#each Sections}}
{{#notFirst ','}}{{#newline}}
        { {{#newline}}
            name: "{{Name}}",{{#newline}}
            id: "{{Id}}",{{#newline}}
            sectionFields: { {{#newline}}
                icon: "{{Icon}}"{{#newline}}
            },{{#newline}}
            fields: [
{{#each Fields}}
{{#notFirst ','}}{{#newline}}
                { {{#newline}}
                    id: "{{Id}}",{{#newline}}
                    name: "{{Name}}",{{#newline}}
                    type: {{Type}},{{#newline}}
                    sortorder: {{SortOrder}}{{#newline}}
                }
{{/each}}{{#newline}}
            ]{{#newline}}
        }
{{/each}}{{#newline}}
    ],{{#newline}}
{{#with StandardValues}}
    standardValues: { {{#newline}}
        id: "{{Id}}",{{#newline}}
        languages: ["en"],{{#newline}}
        fields: { 
{{#each Fields}}
{{#notFirst ','}}{{#newline}}
            "{{Name}}": "{{Value}}"
{{/each}}{{#newline}}
        }{{#newline}}
    }{{#newline}}
{{/with}}
}