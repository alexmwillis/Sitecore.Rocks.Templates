var {{camelCase Name}}Template: SinjTemplateDto = { 
    id: "{{Id}}",
    name: "{{Name}}",
    parent: "{{ParentPath}}",
    fields: { 
        icon: "{{Icon}}",
        "Base template": "{{BaseTemplates}}"
    },
{{#if Sections}}
    sections: [
        {{> sinj-template-sections Sections}}
    ]{{#if StandardValues}},{{/if}}
{{else}}
    sections: []{{#if StandardValues}},{{/if}}
{{/if}}
{{#if StandardValues}}
{{#with StandardValues}}
    standardValues: { 
        id: "{{Id}}",
        languages: ["en"],
        fields: { 
            {{> sinj-fields Fields}}
        }
    }
{{/with}}
{{/if}}
}